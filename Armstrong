package armstrong;

import java.util.Scanner;

/**
 *
 * @author Praktikum
 */
class Armstrong {

    public static void main(String args[]) {
        int n;

        Scanner in = new Scanner(System.in);
        System.out.println("Program ini berjudul Armstrong tapi isinya tentang Ganjil Genap :')");
        System.out.print("\nAngka\t: ");
        n = in.nextInt();

        if (n % 2 == 0) {
            System.out.println("Bilangan Genap");
        } else {
            System.out.println("Bilangan Ganjil");
        }

    }
}
